<h1>Listing lab_results</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Creatinine</th>
      <th>Bilirubin</th>
      <th>Albumin</th>
      <th>Hematocrit</th>
      <th>Glucose</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_results.each do |lab_result| %>
      <tr>
        <td><%= lab_result.date %></td>
        <td><%= lab_result.creatinine %></td>
        <td><%= lab_result.bilirubin %></td>
        <td><%= lab_result.albumin %></td>
        <td><%= lab_result.hematocrit %></td>
        <td><%= lab_result.glucose %></td>
        <td><%= link_to 'Show', [lab_result.patient, lab_result] %></td>
        <td><%= link_to 'Edit', edit_patient_lab_result_path(lab_result.patient, lab_result) %></td>
        <td><%= link_to 'Destroy', [lab_result.patient, lab_result], :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Lab result', new_patient_lab_result_path(params[:patient_id]) %>
<hr>
<%= link_to 'Back', patient_path(params[:patient_id]) %>
